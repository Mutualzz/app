{
    "description": "base",
    "identifier": "base",
    "permissions": [
        "core:path:default",
        "core:event:default",
        "core:window:default",
        "core:window:allow-start-dragging",
        "core:window:allow-close",
        "core:window:allow-minimize",
        "core:window:allow-toggle-maximize",
        "core:window:allow-set-icon",
        "core:image:default",
        "core:app:default",
        "core:resources:default",
        "core:menu:default",
        "core:tray:default",
        "updater:default",
        "notification:default",
        "os:allow-platform",
        "os:allow-arch",
        "os:allow-family",
        "os:allow-locale",
        "os:allow-os-type",
        "os:allow-version",
        "core:webview:allow-internal-toggle-devtools",
        "process:default",
        "process:allow-restart"
    ],
    "platforms": ["linux", "macOS", "windows"],
    "windows": ["main"]
}
